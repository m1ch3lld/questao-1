//Questao 1

programa {
  funcao inicio() {

    real a, b, c, soma
    escreva("Digite o valor de A: ")
    leia(a)
    escreva("Digite o valor de B: ")
    leia(b)
    escreva("Digite o valor de C: ")
    leia(c)

    soma = a + b
    se soma < c entao
    escreva("A soma de a + b e menor que C! ")
    senao 
    escreva("A soma de a + b e menor que c! ")

  }
}
